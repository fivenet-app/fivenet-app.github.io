":132,"value":166},"PHPMyAdmin",{"type":132,"value":168},") and connect to the database your gameserver and FiveNet is using.",{"type":126,"tag":135,"props":170,"children":171},{},[172,174,180],{"type":132,"value":173},"Select the ",{"type":126,"tag":141,"props":175,"children":177},{"className":176},[],[178],{"type":132,"value":179},"fivenet_accounts",{"type":132,"value":181}," table and create a new entry with the following values in it:",{"type":126,"tag":183,"props":184,"children":185},"ul",{},[186,206,246],{"type":126,"tag":187,"props":188,"children":189},"li",{},[190,196,198,204],{"type":126,"tag":141,"props":191,"children":193},{"className":192},[],[194],{"type":132,"value":195},"enabled",{"type":132,"value":197},": ",{"type":126,"tag":141,"props":199,"children":201},{"className":200},[],[202],{"type":132,"value":203},"1",{"type":132,"value":205}," (= enables the account)",{"type":126,"tag":187,"props":207,"children":208},{},[209,215,217,223,224,230,231,237,238,244],{"type":126,"tag":141,"props":210,"children":212},{"className":211},[],[213],{"type":132,"value":214},"license",{"type":132,"value":216},": Your in-game identifier/license without any prefix, such as, ",{"type":126,"tag":141,"props":218,"children":220},{"className":219},[],[221],{"type":132,"value":222},"char*:",{"type":132,"value":159},{"type":126,"tag":141,"props":225,"children":227},{"className":226},[],[228],{"type":132,"value":229},"license:",{"type":132,"value":159},{"type":126,"tag":141,"props":232,"children":234},{"className":233},[],[235],{"type":132,"value":236},"steam:",{"type":132,"value":159},{"type":126,"tag":141,"props":239,"children":241},{"className":240},[],[242],{"type":132,"value":243},"discord:",{"type":132,"value":245}," and so on.",{"type":126,"tag":187,"props":247,"children":248},{},[249,255],{"type":126,"tag":141,"props":250,"children":252},{"className":251},[],[253],{"type":132,"value":254},"reg_token",{"type":132,"value":256},": Must be 6 numbers long. This will be your registration token which you need to register your account.",{"type":126,"tag":135,"props":258,"children":259},{},[260],{"type":132,"value":261},"(All other fields must be left as they are)",{"type":126,"tag":135,"props":263,"children":264},{},[265],{"type":132,"value":266},"Now open your FiveNet instance in your browser, navigate to the registration page and use your chosen registration token to create your account.",{"type":126,"tag":135,"props":268,"children":269},{},[270,272,277,279,285,287,292,293,299],{"type":132,"value":271},"You should now be able to successfully login to FiveNet and if the ",{"type":126,"tag":141,"props":273,"children":275},{"className":274},[],[276],{"type":132,"value":214},{"type":132,"value":278}," value of the account is correct see any characters (",{"type":126,"tag":141,"props":280,"children":282},{"className":281},[],[283],{"type":132,"value":284},"users",{"type":132,"value":286}," table) that are associated with the ",{"type":126,"tag":141,"props":288,"children":290},{"className":289},[],[291],{"type":132,"value":214},{"type":132,"value":118},{"type":126,"tag":141,"props":294,"children":296},{"className":295},[],[297],{"type":132,"value":298},"identifier",{"type":132,"value":300},".",{"type":126,"tag":127,"props":302,"children":304},{"id":303},"_2-add-fivenet-plugin-to-your-fivem-gameserver",[305],{"type":132,"value":306},"2. Add FiveNet Plugin to your FiveM gameserver",{"type":126,"tag":135,"props":308,"children":309},{},[310,312,319,321,327],{"type":132,"value":311},"The plugin code can be found ",{"type":126,"tag":150,"props":313,"children":316},{"href":314,"rel":315},"https://github.com/fivenet-app/plugins/tree/main/fivem/fivenet",[154],[317],{"type":132,"value":318},"here",{"type":132,"value":320},". Make sure to read the ",{"type":126,"tag":141,"props":322,"children":324},{"className":323},[],[325],{"type":132,"value":326},"README.md",{"type":132,"value":328}," and work through the instructions to get the plugin running optimally.",{"type":126,"tag":330,"props":331,"children":332},"callout",{},[333],{"type":126,"tag":135,"props":334,"children":335},{},[336],{"type":132,"value":337},"You must add event triggers to your ESX plugins accordingly, as not all events that FiveNet collects are available in the standard ESX plugin codes.",{"title":117,"searchDepth":339,"depth":339,"links":340},2,[341,342],{"id":129,"depth":339,"text":133},{"id":303,"depth":339,"text":306},"markdown","content:1.getting-started:4.installation:6.post-install-steps.md","content","1.getting-started/4.installation/6.post-install-steps.md","1.getting-started/4.installation/6.post-install-steps","md",[350,352],{"_path":43,"title":42,"description":351},"For installation to Kubernetes a Helm chart is available in our charts repository. The source code of the chart(s) is available on GitHub fivenet-app/charts.",{"_path":53,"title":52,"description":117},1734009306998]